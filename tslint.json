{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "always"
        },
        "esSpecCompliant": true
      }
    ],
    "quotemark": [ true, "single" ],
    "semicolon": [ true, "always" ],
    "no-console": [ false, "log", "warn" ],
    "newline-before-return": true,
    "array-type": [ true, "generic" ],
    "max-line-length": [
      true,
      {
        "limit": 150,
        "ignore-pattern": "^import |^export {(.*?)}",
        "check-strings": true,
        "check-regex": true
      }
    ],
    "arrow-parens": [ false ],
    "object-literal-sort-keys": [ false ]
  },
  "rulesDirectory": []
}
